@model IEnumerable<AdvanceTourismGuide.Models.Hotel_Booking>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/AdminLayoutPage.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Place_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Approve_Status)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Coment)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Place_Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Phone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Approve_Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Coment)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.HB_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.HB_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.HB_Id })
        </td>
    </tr>
}

</table>
