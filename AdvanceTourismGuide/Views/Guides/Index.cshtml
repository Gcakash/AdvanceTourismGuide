@model IEnumerable<AdvanceTourismGuide.Models.Guide>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/AdminLayoutPage.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Place_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Guide_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Guide_Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Guide_Pic)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Guide_Contac)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Place_Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Guide_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Guide_Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Guide_Pic)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Guide_Contac)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Guide_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Guide_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Guide_Id })
        </td>
    </tr>
}

</table>
