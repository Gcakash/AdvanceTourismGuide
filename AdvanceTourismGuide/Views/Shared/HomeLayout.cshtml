<!DOCTYPE html>

<html>
<head>


    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>


    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css/style.css" rel="stylesheet" />
    <link href="~/Content/css/fontawesome.min.css" rel="stylesheet" />
    
    <script src="~/js/jquery-3.3.1.min.js"></script>
    

    <script src="~/js/bootstrap.min.js"></script>
   
    <link href="https://fonts.googleapis.com/css?family=Open+Sans|Roboto" rel="stylesheet">



</head>
<body>
   
    
    

    <div id="header" class="header">
        <nav class="navbar navbar-expand-lg navbar-light text-capitalize">
            <div class="container">
                <a class="navbar-brand" href="#"><img src="~/Content/images/logo.png" alt="#" /></a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#show-menu" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="show-menu">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item active">
                            <a class="nav-link" href="~/Home/HomeIndex">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="~/Home/About">About</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="~/Home/Services">Service</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="~/Home/Contact">Contact</a>
                        </li>
                        <li class="nav-item">
                            <a  class="nav-link" href="~/UserInfoes/Signup"><span class="glyphicon glyphicon-user"></span> Sign Up</a>
                        </li>
                        <li class="nav-item dropdown">
                            <a  class="dropdown-toggle glyphicon glyphicon-log-in nav-link" data-toggle="dropdown" href="#">  Login <span class="caret"></span></a>
                            <ul class="dropdown-menu "style="background-color:seagreen;font-size:17px">
                                <li><a class="nav-link" href="~/UserInfoes/Login">User Login</a></li>
                                <li><a  class="nav-link" href="~/AdminInfoes/AdminLogin">Admin Login</a></li>

                            </ul>
                        </li>
                       
                    </ul>
                </div>
            </div>
        </nav>
    </div>




    <div>
        @RenderBody()
    </div>


    <footer>
        <div class="container">
            <div class="row">
                <div class="col-lg-3 col-md-6 col-12">
                    <div class="footer_blog_section">
                        <img src="~/Content/images/logo.png" alt="#" />
                        <p style="margin-top: 5px;">We are here to work in various sector and we will try to provide better services</p>
                    </div>
                </div>
                <div class="col-lg-2 col-md-6 col-12">
                    <div class="item">
                        <h4 class="text-uppercase">Navigation</h4>
                        <ul>
                            <li><a href="#">Home</a></li>
                            <li><a href="#">About</a></li>

                            <li><a href="#">Service</a></li>

                        </ul>
                    </div>
                </div>
                <div class="col-lg-4 col-md-6 col-12">
                    <div class="item">
                        <h4 class="text-uppercase">Contact Info</h4>
                        <p><strong> Office Address:</strong></p>
                        <p><img src="~/Content/images/phone_icon.png" alt="#" />9867288224 butwal Rupandehi</p>
                        <p><strong>Customer Service:</strong></p>
                        <p> 9867288224 butwal Rupandehi</p>
                    </div>
                </div>


                <div class="col-lg-3 col-md-6 col-12">
                    <div class="item">
                        <h4 class="text-uppercase">Social</h4>
                        <ul>
                            <li> <a href="https://www.facebook.com/"> Facebook </a> </li>
                            <li> <a href="https://twitter.com/"> Twitter </a> </li>
                            <li> <a href="https://plus.google.com/"> Google+ </a> </li>
                        </ul>
                    </div>
                </div>

            </div>
        </div>
        <div class="copyright text-center">
            <p>Copyright 2019  Design by sky group</p>
        </div>
    </footer>





</body>
</html>
